{
  "compilerOptions": {
    "strict": false,
    "baseUrl": "./src",
    "outDir": "dist",
    "target": "ESNext",
    "module": "CommonJS",
    "declaration": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "types": [
      "node"
    ]
  },
  "exclude": ["node_modules", "dist"]
}
